<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<!-- type의 값에 경로를 적음. -->
<configuration>
	<settings>
		<!-- 오라클 컬럼이 first_name 를 VO firstName 처럼 카멜케이스로 변환 -->
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>

	<typeAliases>
		<typeAlias type="com.cbt.company.CompanyVO" alias="CompanyVO" />
		
		<!-- candidate alias 연결                 6/28 김재용 -->
		<typeAlias type="com.cbt.candidate.CandidateVO" alias="CandidateVO" />
	</typeAliases>
</configuration>


















